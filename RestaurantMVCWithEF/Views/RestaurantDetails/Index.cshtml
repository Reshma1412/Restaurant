@model IEnumerable<RestaurantMVCWithEF.Models.RestaurantDetail>

@{
    ViewBag.Title = "Index";
}

<h2>Restaurant Details</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RestuarantName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RestaurantAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MobileNo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RestuarantName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RestaurantAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MobileNo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.RestuarantID }) |
            @Html.ActionLink("Details", "Details", new { id=item.RestuarantID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.RestuarantID })
        </td>
    </tr>
}

</table>
